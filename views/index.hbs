<!DOCTYPE html>
<html>
  <head>
    <title>Virtual Sensors</title>
    <link href="/public/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/public/dash.css">
    <link rel=icon type="image/png" href="/public/images/bluemix-icon.png">
  </head>
  <body>
    <nav class="navbar navbar-default navbar-fixed-top">
      <a class="navbar-brand" rel="home" href="#" title="Virtual Sensors">
         <img style="max-width:35px;margin-top:-10px;margin-right:-15px;"
          src="/public/images/bluemix-icon.png">
      </a>
      <a class="navbar-brand" href="/">Virtual Sensors</a>
    </nav>
    {{#each cards}}
      <div class="outlinebox">
        <div class="outlinecontrols">
          <h1>{{name}}</h1>
          <div class="uparrow">&#x25B2;</div>
          <div class="numberval">{{value}}</div>
          <div class="downarrow">&#x25BC;</div>
        </div>
        <div class="outlinefooter">
        </div>
      </div>
    {{/each}}
    <div class="outlinebox newoutlinebox">
      <div class="outlinecontrols">
        <h1>new</h1>
        <div class="numberval" style="font-size:120px;margin-top:-40px">+</div>
      </div>
      <div class="outlinefooter">
      </div>
    </div>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="/public/bootstrap/js/bootstrap.min.js"></script>
  </body>
</html>
